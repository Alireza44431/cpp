#include<iostream>
#include<conio.h>
#include<string.h>
using namespace std;
int main()
{
	int x=0;
	cout<<"plese innput your number : ";
	cin>>x;
	
	
		for(int i=1;i<x+1;i++)
		{
			if (i % 5 != 0)
			{
				cout<<i<<endl;
			}
			else if (i%5==0)
			{
				cout<<"hope"<<endl;
			}
		}
	
	getch();
	return 0;
}
